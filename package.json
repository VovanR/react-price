{
  "name": "react-price",
  "version": "0.6.1",
  "description": "React price component",
  "license": "MIT",
  "repository": "VovanR/react-price",
  "author": "Vladimir Rodkin <mail@vovanr.com> (https://github.com/VovanR)",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p",
    "start": "webpack --watch",
    "test": "xo && ava --verbose",
    "release-patch": "mversion patch",
    "release-minor": "mversion minor",
    "release-major": "mversion major",
    "deploy": "npm run build && gh-pages -d example"
  },
  "files": [
    "index.js",
    "style.css"
  ],
  "main": "index.js",
  "style": "style.css",
  "keywords": [
    "react",
    "component",
    "react-component",
    "price",
    "cost",
    "money",
    "cash",
    "currency"
  ],
  "dependencies": {},
  "devDependencies": {
    "@ava/babel": "1.0.1",
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@babel/register": "7.8.3",
    "ava": "3.3.0",
    "babel-loader": "8.0.6",
    "conventional-changelog-cli": "2.0.31",
    "cross-env": "7.0.0",
    "css-loader": "3.4.2",
    "cz-conventional-changelog": "3.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-xo": "0.29.0",
    "eslint-config-xo-react": "0.23.0",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "gh-pages": "2.2.0",
    "husky": "4.2.3",
    "mversion": "1.13.0",
    "prop-types": "15.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "style-loader": "1.1.3",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "xo": "0.26.1"
  },
  "peerDependencies": {
    "prop-types": "*",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "ava": {
    "babel": true,
    "require": [
      "@babel/register",
      "./test/helpers/setup-test-env.js"
    ]
  },
  "xo": {
    "extends": "xo-react",
    "space": true
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
