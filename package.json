{
  "name": "react-price",
  "version": "0.5.1",
  "description": "React price component",
  "license": "MIT",
  "repository": "VovanR/react-price",
  "author": {
    "name": "Vladimir Rodkin",
    "email": "mail@vovanr.com",
    "url": "https://github.com/VovanR"
  },
  "scripts": {
    "prepush": "npm test",
    "test": "xo && ava --verbose",
    "start": "webpack --watch",
    "release-patch": "mversion patch",
    "release-minor": "mversion minor",
    "release-major": "mversion major",
    "build": "webpack",
    "deploy": "gh-pages-deploy"
  },
  "files": [
    "index.jsx",
    "style.css"
  ],
  "main": "index.jsx",
  "style": "style.css",
  "keywords": [
    "react",
    "component",
    "react-component",
    "price",
    "cost",
    "money",
    "cash",
    "currency"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "0.17.0",
    "babel-loader": "6.2.8",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.18.0",
    "conventional-changelog-cli": "1.2.0",
    "css-loader": "0.26.0",
    "cz-conventional-changelog": "1.2.0",
    "enzyme": "2.6.0",
    "eslint": "3.10.2",
    "eslint-config-xo": "0.17.0",
    "eslint-config-xo-react": "0.10.0",
    "eslint-plugin-react": "6.7.1",
    "husky": "0.11.9",
    "jsdom": "9.8.3",
    "mversion": "1.10.1",
    "react": "15.4.1",
    "react-addons-test-utils": "15.4.1",
    "react-dom": "15.4.1",
    "style-loader": "0.13.1",
    "webpack": "1.13.3",
    "xo": "0.17.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "./test/helpers/setup-test-env.js"
    ]
  },
  "xo": {
    "extends": "xo-react"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gh-pages-deploy": {
    "staticpath": "example",
    "prep": [
      "build"
    ],
    "noprompt": false
  }
}
